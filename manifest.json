{
  "manifest_version": 3,
  "name": "My Startpage",
  "short_name": "Startpage",
  "version": "1.0.1",
  "description": "自定义主页 + 扁平化书签，替换新标签页",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "action": {
    "default_title": "My Startpage"
  }
}

